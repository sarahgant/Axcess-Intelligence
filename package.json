{
  "version": "1.0.0",
  "source": "./index.html",
  "type": "module",
  "name": "cch-axcess-intelligence-vibed",
  "description": "CCH Axcess Intelligence Vibed - AI-powered document processing and tax research platform",
  "_architecture_reminder": "ðŸš¨ ENV FILES EXIST - NEVER CREATE/MODIFY THEM! See docs/ARCHITECTURE_CONSTRAINTS.md",
  "scripts": {
    "dev": "npm run clean && concurrently --kill-others-on-fail \"npm run frontend\" \"npm run backend\"",
    "start": "npm run clean && concurrently --kill-others-on-fail \"npm run frontend\" \"npm run backend\"",
    "frontend": "vite",
    "backend": "cd server && npm run dev",
    "clean": "kill-port 5173 3001 3002 4173 3000 5000 8080 8000",
    "start:production": "npm run client:build && node server/server.js",
    "build": "npm run client:build && npm run server:install",
    "client:dev": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "fresh": "node scripts/fresh-install.cjs",
    "fresh:windows": "npm run clean && rmdir /s /q node_modules && del package-lock.json && npm install",
    "kill-ports": "powershell -ExecutionPolicy Bypass -File scripts/kill-ports.ps1",
    "ports:check": "node scripts/check-ports.cjs",
    "ports:kill": "npm run clean",
    "doctor": "npm run ports:check && npm list --depth=0",
    "server": "npm run server:dev",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "security:audit": "npm audit && npm run security:check-deps",
    "security:check-deps": "npm ls --depth=0",
    "security:fix": "npm audit fix",
    "security:fix-force": "npm audit fix --force",
    "security:check-env": "node scripts/check-env-security.cjs",
    "lint:security": "eslint src/ --config .eslintrc.security.js",
    "test:security": "npm run test -- --testPathPattern=security",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "setup:env": "node scripts/setup-env.js",
    "setup:production": "node setup-production-env.cjs"
  },
