{
  "version": "1.0.0",
  "source": "./index.html",
  "type": "module",
  "name": "cch-axcess-intelligence-vibed",
  "description": "CCH Axcess Intelligence Vibed - AI-powered document processing and tax research platform",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm run dev",
    "dev:clean": "node scripts/start-dev.cjs",
    "dev:all": "node scripts/start-dev.cjs",
    "start": "npm run start:production",
    "start:production": "concurrently \"npm run server:start\" \"npm run client:build && npm run client:preview\"",
    "run-app": "node run-app.cjs",
    "start:clean": "node scripts/start-clean.cjs",
    "start:clean:windows": "powershell -ExecutionPolicy Bypass -File start-clean.ps1",
    "build": "npm run client:build && npm run server:install",
    "client:dev": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "clean": "kill-port 5173 3001 3002 4173 3000 5000 8080 8000",
    "fresh": "node scripts/fresh-install.cjs",
    "fresh:windows": "npm run clean && rmdir /s /q node_modules && del package-lock.json && npm install",
    "kill-ports": "powershell -ExecutionPolicy Bypass -File scripts/kill-ports.ps1",
    "ports:check": "node scripts/check-ports.cjs",
    "ports:kill": "npm run clean",
    "doctor": "npm run ports:check && npm list --depth=0",
    "server": "npm run server:dev",
    "preview": "vite preview",
    "test": "echo 'Running tests...' && npm run test:unit",
    "test:unit": "echo 'Unit tests would run here'",
    "test:integration": "echo 'Integration tests would run here'",
    "test:e2e": "echo 'E2E tests would run here'",
    "lint": "echo 'ESLint would run here'",
    "lint:fix": "echo 'ESLint fix would run here'",
    "ci": "npm run lint && npm run test && npm run client:build",
    "security:audit": "npm audit --audit-level moderate",
    "security:check-deps": "npm ls --depth=0",
    "security:fix": "npm audit fix",
    "security:fix-force": "npm audit fix --force",
    "security:check-env": "node scripts/check-env-security.cjs",
    "lint:security": "eslint src/ --config .eslintrc.security.js",
    "test:security": "npm run test -- --testPathPattern=security",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "jest tests/accessibility",
    "test:performance": "jest tests/performance",
    "test:debug": "jest --inspect-brk --runInBand",
    "test:clear-cache": "jest --clearCache",
    "coverage:open": "open coverage/lcov-report/index.html",
    "coverage:check": "jest --coverage --coverageThreshold",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@wk/theme": "^2.42.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.1",
    "dotenv": "^17.2.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.453.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.1",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "1.0.7",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@vitejs/plugin-react": "4.3.4",
    "chalk": "^4.1.2",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.8",
    "globals": "15.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "kill-port": "^2.0.1",
    "msw": "^2.10.4",
    "playwright": "^1.54.2",
    "ps-tree": "^1.2.0",
    "sass": "^1.89.2",
    "tailwindcss": "3.4.16",
    "ts-jest": "^29.4.1",
    "vite": "^6.3.5"
  },
  "alias": {
    "@/*": "./src/components/ui/$1"
  }
}
